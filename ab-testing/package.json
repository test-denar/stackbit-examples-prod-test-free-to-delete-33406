{
    "name": "personalization-next-contentful-netlify-stackbit",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm-run-all -p cache-content:dev next:dev",
        "build": "npm run cache-content && next build",
        "start": "next start",
        "lint": "next lint",
        "next:dev": "next dev",
        "cache-content": "ts-node ./src/scripts/cache-content.ts",
        "cache-content:dev": "nodemon --watch src -e ts,tsx,txt --exec \"ts-node ./src/scripts/cache-content.ts\"",
        "export-content": "node ./contentful/export.js"
    },
    "dependencies": {
        "@netlify/next": "^1.4.8",
        "@swc/core": "^1.3.64",
        "@types/node": "20.3.1",
        "@types/prettier": "^2.7.3",
        "@types/react": "18.2.12",
        "@types/react-dom": "18.2.5",
        "autoprefixer": "10.4.14",
        "classnames": "^2.3.2",
        "contentful": "^10.2.3",
        "contentful-management": "^10.36.0",
        "dotenv": "^16.1.4",
        "eslint": "8.42.0",
        "eslint-config-next": "13.4.5",
        "marked": "^5.1.0",
        "next": "13.1.1",
        "nodemon": "^2.0.22",
        "npm-run-all": "^4.1.5",
        "postcss": "8.4.24",
        "prettier": "^2.8.8",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-portal": "^4.2.2",
        "tailwindcss": "3.3.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.1.3"
    },
    "devDependencies": {
        "@stackbit/cms-contentful": "^0.3.15",
        "@stackbit/types": "^0.7.6",
        "@types/marked": "^5.0.0",
        "@types/react-portal": "^4.0.4",
        "contentful-export": "^7.19.59",
        "contentful-import": "^8.5.57"
    }
}
